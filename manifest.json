	{
	"manifest_version": 2,
	"name": "Olymp Auto Trade",
	"description": "Trading automático no Olymp",
	"version":"1.0",
	"permissions": [
	  "tabs"
	],
	"content_scripts": [
	{
		"matches": [
			"https://olymptrade.com/platform"
		],
		"js":["content.js"],
		"run_at": "document_end"
	}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup" : "index.html",
		"default_title" : "Configure Extension"
	}
}